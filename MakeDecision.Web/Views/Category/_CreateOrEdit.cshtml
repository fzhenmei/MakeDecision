@model MakeDecision.Web.Models.Category

@* This partial view defines form fields that will appear when creating and editing entities *@

<div class="editor-label">
    所在部门名称：@Model.Department.DepartmentName
    @Html.HiddenFor(model => model.DepartmentId)
</div>
<div class="editor-label">
    @Html.LabelFor(model => model.CategoryName)
</div>
<div class="editor-field">
    @Html.EditorFor(model => model.CategoryName)
    @Html.ValidationMessageFor(model => model.CategoryName)
</div>
<div class="editor-label">
    @Html.LabelFor(model => model.CycleId)
</div>
<div class="editor-field">
    @Html.DropDownList("CycleId", String.Empty)
    @Html.ValidationMessageFor(model => model.CycleId)
</div>
<div class="editor-label">
    @Html.LabelFor(model => model.UnitId)
</div>
<div class="editor-field">
    @Html.DropDownList("UnitId", String.Empty)
    @Html.ValidationMessageFor(model => model.UnitId)
</div>
